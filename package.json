{
    "name": "@rymote/pulse-client",
    "version": "2.0.0",
    "author": "Rymote <hi@rymote.io> (https://rymote.io/)",
    "private": false,
    "funding": [
        {
            "type": "patreon",
            "url": "https://patreon.com/rymote"
        },
        {
            "type": "opencollective",
            "url": "https://opencollective.com/rymote"
        }
    ],
    "license": "MIT",
    "description": "TypeScript client library for Rymote.Pulse with real-time communication (RTC) support",
    "type": "module",
    "main": "dist/index.js",
    "module": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "format": "prettier --write ./",
        "example:rtc": "tsc && node dist/examples/rtc-example.js",
        "dev": "tsc --watch",
        "prepare": "npm run build"
    },
    "keywords": [
        "rymote",
        "pulse",
        "websocket",
        "realtime",
        "typescript"
    ],
    "devDependencies": {
        "@types/node": "^22.14.1",
        "@types/ws": "^8",
        "prettier": "^3.5.3",
        "typescript": "^5.5.3"
    },
    "dependencies": {
        "@stablelib/chacha20poly1305": "^2.0.1",
        "msgpackr": "^1.11.2"
    },
    "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
